{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-console": "off",
    "no-param-reassign": "off",
    "func-names": "off",
    "consistent-return": "off",
    "no-underscore-dangle": "off",
    "no-return-await": "off",
    "implicit-arrow-linebreak": "off",
    "operator-linebreak": "off"
  }
}
